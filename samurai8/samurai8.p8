--samurai 8
--by panoni

--datajank
attackvalues=
{
 attackduration=10,
 attackdelay=20
}
g=
{
	grav=0.4, -- gravity per frame
}

clashvalues={
	clashduration=10,
	clashtimer=10,
	isclashing=false,
}

currentstage=1

mapvalues={
	x=0,
	y=0,
	mapw=128,
	maph=128,
}

stages={
	{0,0},--stage1
	{19,0},--stage2
	{39,0},
	{58,0},
	{77,0},
	{96,0},
	{114,0},
	{0,16},
	{19,16},
	{39,16}
}

--functionjank

function death(player)
	player.alive=false
	player.lives-=1
	player.x=player.startx
	player.y=player.starty
	player.dy=0
	player.dx=0
	sfx(004)
	player.alive=true
end

function attacklocator(player)
  if player.isleft then
	player.attacklocationx = player.x - 8
  else
	player.attacklocationx = player.x + 8
  end
  if player.attackingup then
	player.attacklocationy=player.y-8
  else
	player.attacklocationy=player.y
end
end

function attacktimer(player)
	if (player.isattacking and player.attackduration > 0) then
		player.attackduration -= 1
		else if (player.isattacking and player.attackduration <= 0) then
		 player.isattacking = false
		 player.attackduration = attackvalues.attackduration
		 player.canattack = false
		end
	end
	   
	if (not player.canattack and player.attackdelay>0) then
		player.attackdelay -= 1
		else if (not player.canattack and player.attackdelay<=0) then
		 player.canattack=true
		 player.attackdelay=attackvalues.attackdelay
		end
	end
end

function hitdetection(attacker,defender)
	if (attacker.isattacking and not clashvalues.isclashing and defender.alive) then
		if ((abs(attacker.attacklocationy - defender.y) < 8) and (abs(attacker.attacklocationx - defender.x) < 8 )) then
			death(defender)
		end
	end
end

function buttonchecker(player,controls)
	if (btnp(4,controls))
	and player.isgrounded and not clashvalues.isclashing then
	--launch the player upwards
	   player.dy=-player.jumpvel
   end
   if (btnp(5,controls) and not player.isattacking and player.canattack) then
	   player.isattacking = true
	   if player.aimingup then 
		player.attackingup=true
	   else 
		player.attackingup=false 
		end
	   sfx(00)
   end
   if btn(2,controls) then
	player.aimingup=true
   else
	player.aimingup=false
   end
end

function clash()
	if not clashvalues.isclashing then sfx(003) end
	clashvalues.isclashing=true
	if clashvalues.clashtimer > 0 then
		if clashvalues.clashtimer == clashvalues.clashduration then
			if p1.isleft then 
				p1.dx=3
			else
				p1.dx=-3
			end
			if p2.isleft then 
				p2.dx=3
			else
				p2.dx=-3
			end
		end
		clashvalues.clashtimer-=1
		local p1startx=p1.x
		local p2startx=p2.x

		p1.x+=p1.dx
		p2.x+=p2.dx

		local p1xoffset=0
		local p2xoffset=0
		if p1.dx>0 then p1xoffset=7 end
		if p2.dx>0 then p2xoffset=7 end
		
		--look for a wall
		local h1=mget((p1.x+p1xoffset)/8 + (mapvalues.x * 8),(p1.y+7)/8)
		if fget(h1,0) then
			--they hit a wall so move them
			--back to their original pos.
			--it should really move them to
			--the edge of the wall but this
			--mostly works and is simpler.
			p1.x=p1startx
			if p1.dx > 0 then
				p1.dx-=2
			else
				p1.dx+=2
			end
			sfx(005)
			p1.dx=-p1.dx
		end
	
		local h2=mget((p2.x+p2xoffset)/8,(p2.y+7)/8)
		if fget(h2,0) then
			p2.x=p2startx
			if p2.dx > 0 then
				p2.dx-=2
			else
				p2.dx+=2
			end
			sfx(005)
			p2.dx=-p2.dx
		end
	else
		clashvalues.isclashing=false
		clashvalues.clashtimer=clashvalues.clashduration
	end
end

function gameovercheck()
	if p1.lives < 0 then
		state="gameover"
	else if p2.lives < 0 then
		state="gameover"
	end
	end
	endscreentimer=60
end

function falldeathcheck(player)
	if player.y>129+mapyoffset then
		death(player)
	end
end

function print_centered(str,y,color)
	print(str, 64 - (#str * 2), y,color) 
end

--running the game
function _init()
	currentstage=1
	music(0,0,0)

	state="menu"
	p1={
		startx=90,
		starty=16,
		x=90,
		y=16,
		dx=0,
		dy=0,
		isgrounded=false,
		isleft=true,
		jumpvel=5,
		isattacking=false,
		attackduration=attackvalues.attackduration,
		canattack=true,
		attackdelay=attackvalues.attackdelay,
		attacklocationx=0,
		attacklocationy=0,
		aimingup=false,
		attackingup=false,
		alive=true,
		lives=3}
	p2={
		startx=32,
		starty=16,
		x=32,
		y=16,
		dx=0,
		dy=0,
		isgrounded=false,
		isleft=false,
		jumpvel=5,
		isattacking=false,
		attackduration=attackvalues.attackduration,
		canattack=true,
		attackdelay=attackvalues.attackdelay,
		attacklocationx=0,
		attacklocationy=0,
		aimingup=false,
		attackingup=false,
		alive=true,
		lives=3}
	
end

-- called 30 times per second
function updategame()
	gameovercheck()
	falldeathcheck(p1)
	falldeathcheck(p2)
	--remember where we started
	local p1startx=p1.x
	local p2startx=p2.x
	buttonchecker(p1,0)
	buttonchecker(p2,1)

	--walk
	--
	--p1
	if not clashvalues.isclashing then
		p1.dx=0
		if btn(0) then --left
			p1.dx=-2.5
			if (not p1.isattacking) then
			 p1.isleft=true
			end
		end
		if btn(1) then --right
			p1.dx=2.5
			if (not p1.isattacking) then
			 p1.isleft = false
			end
		end
	
		--p2
		p2.dx=0
		if btn(0,1) then --left
			p2.dx=-2.5
			if (not p2.isattacking) then
			 p2.isleft=true
			end
		end
		if btn(1,1) then --right
			p2.dx=2.5
			if (not p2.isattacking) then
			 p2.isleft = false
			end
		end
	end
	
	--move the player left/right
	if not clashvalues.isclashing then
		p1.x+=p1.dx
		p2.x+=p2.dx
	end
	
	--hit side walls
	--
	
	--check for walls in the
	--direction we are moving.
	local p1xoffset=0
	local p2xoffset=0
	if p1.dx>0 then p1xoffset=7 end
	if p2.dx>0 then p2xoffset=7 end

	--look for a wall
	local h1=mget((p1.x+p1xoffset)/8,(p1.y+7)/8)
	if fget(h1,0) then
		--they hit a wall so move them
		--back to their original pos.
		--it should really move them to
		--the edge of the wall but this
		--mostly works and is simpler.
		p1.x=p1startx
	end

	local h2=mget((p2.x+p2xoffset)/8 ,(p2.y+7)/8)
	if fget(h2,0) then
		p2.x=p2startx
	end
	
	--accumulate gravity
	p1.dy+=g.grav
	p2.dy+=g.grav
	
	--fall
	p1.y+=p1.dy
    p2.y+=p2.dy
	--hit floor
	--
	
	--check bottom center of the
	--player.
	local v1=mget((p1.x+4)/8,(p1.y+8)/8)
	
	--assume they are floating 
	--until we determine otherwise
	p1.isgrounded=false
	
	--only check for floors when
	--moving downward
	if p1.dy>=0 then
		--look for a solid tile
		if fget(v1,0) then
			--place p1 on top of tile
			p1.y = flr((p1.y)/8)*8
			--halt velocity
			p1.dy = 0
			--allow jumping again
			p1.isgrounded=true
		end
	end
	--p2
	local v2=mget((p2.x+4)/8,(p2.y+8)/8)

	p2.isgrounded=false

	if p2.dy>=0 then
		--look for a solid tile
		if fget(v2,0) then
			--place p2 on top of tile
			p2.y = flr((p2.y)/8)*8
			--halt velocity
			p2.dy = 0
			--allow jumping again
			p2.isgrounded=true
		end
	end
	
	--hit ceiling
	--
	
	--check top center of p1
	v1=mget((p1.x+4)/8,(p1.y)/8)
	
	--only check for ceilings when
	--moving up
	if p1.dy<=0 then
		--look for solid tile
		if fget(v1,0) then
			--position p1 right below
			--ceiling
			p1.y = flr((p1.y+8)/8)*8
			--halt upward velocity
			p1.dy = 0
		end
	end

	--p2
	v2=mget((p2.x+4)/8,(p2.y)/8)
	
	--only check for ceilings when
	--moving up
	if p2.dy<=0 then
		--look for solid tile
		if fget(v2,0) then
			--position p2 right below
			--ceiling
			p2.y = flr((p2.y+8)/8)*8
			--halt upward velocity
			p2.dy = 0
		end
	end

	attacklocator(p1)
	attacklocator(p2)
	
	attacktimer(p1)
	attacktimer(p2)

	--clash detetction
	if(p1.isattacking and p2.isattacking) then
		if ((abs(p1.attacklocationx - p2.attacklocationx) < 8) and abs(p1.attacklocationy - p2.attacklocationy) < 8) then
			clash()
		end
	end

	if clashvalues.isclashing then
		clash()
	end
	
	--hit detection
	hitdetection(p1,p2)
	hitdetection(p2,p1)
end



function drawgame()
 cls()
 camera(mapxoffset,mapyoffset)
 map()
 --draw lives
 for i=1, p1.lives do
		spr(016,64+i*8+mapxoffset,4+mapyoffset)
 end
 
 for i=1, p2.lives do
		spr(032,56-i*8+mapxoffset,4+mapyoffset,1,1,true)
 end

 --p1
	if (p1.isgrounded and not p1.isattacking) then
	 spr(017,p1.x,p1.y,1,1,p1.isleft) --player idle
	  else if (p1.isgrounded and p1.isattacking) then
	   spr(019,p1.x,p1.y,1,1,p1.isleft)
	  else if (not p1.isattacking) then
	   spr(018,p1.x,p1.y,1,1,p1.isleft)
	  else if (p1.isattacking) then
	    spr(020,p1.x,p1.y,1,1,p1.isleft)
	   end
	  end
	 end
	end
	
	if p1.isattacking and p1.attackingup then
		spr(006,p1.attacklocationx,p1.attacklocationy,1,1,p1.isleft)
		spr(005,p1.attacklocationx,p1.y,1,1,p1.isleft)
	end

	if p1.isattacking and not p1.attackingup then
		spr(007,p1.attacklocationx,p1.y,1,1,p1.isleft)
	end
	

	--p2
	if (p2.isgrounded and not p2.isattacking) then
		spr(033,p2.x+1,p2.y,1,1,p2.isleft) --player idle
		 else if (p2.isgrounded and p2.isattacking) then
		  spr(035,p2.x+1,p2.y,1,1,p2.isleft)
		 else if (not p2.isattacking) then
		  spr(034,p2.x+1,p2.y,1,1,p2.isleft)
		 else if (p2.isattacking) then
		   spr(036,p2.x+1,p2.y,1,1,p2.isleft)
		  end
		 end
		end
	   end
	if p2.isattacking and p2.attackingup then
		spr(006,p2.attacklocationx+1,p2.attacklocationy,1,1,p2.isleft)
		spr(005,p2.attacklocationx+1,p2.y,1,1,p2.isleft)
	end
	if p2.isattacking and not p2.attackingup then
		spr(007,p2.attacklocationx+1,p2.y,1,1,p2.isleft)
	end
	--death sprites
	if(not p2.alive) then
		spr(022,p2.x,p2.y,1,1,p2.isleft)
	end
	if(not p1.alive) then
		spr(022,p1.x,p1.y,1,1,p1.isleft)
	end
end
