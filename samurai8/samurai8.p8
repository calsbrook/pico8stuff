--menu stuff

function updatemenu()
	if btnp(4) or btnp(4,1) then

		sfx(0)
		sfx(003)

		mapxoffset=mapvalues.x*8
		mapyoffset=mapvalues.y*8

		p1.startx=p1.startx+mapxoffset
		p1.x=p1.startx
		p1.starty=p1.starty+mapyoffset
		p1.y=p1.starty

		p2.startx=p2.startx+mapxoffset
		p2.x=p2.startx
		p2.starty=p2.starty+mapyoffset
		p2.y=p2.starty

		state="game"
	end
	if (btnp(➡️) or btnp(➡️,1) )and currentstage<#stages then
		currentstage+=1
	end
	if (btnp(⬅️) or btnp(⬅️,1)) and currentstage>1 then
		currentstage-=1
	end
	mapvalues.x=stages[currentstage][1]
	mapvalues.y=stages[currentstage][2]
end

function drawmenu()
	cls()
	map(mapvalues.x,mapvalues.y,0,0,120,120,1)
	for i=0,6 do
 		spr(64+i,38+i*8,20)
 		spr(80+i,38+i*8,28)
 		spr(96+i,38+i*8,36)
 		spr(112+i,38+i*8,44)
	end
	print_centered("by panoni",53,9)
	print_centered("⬅️ stage "..currentstage.." ➡️  ",77,7)
	print_centered("press ❎ to start",87,7)

end
