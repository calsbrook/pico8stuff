function updategameover()
	if endscreentimer > 0 then
		endscreentimer-=1
	else
		if btnp(❎) or btnp(🅾️) or btnp(❎,1) or btnp(🅾️,1) then
			_init()
		end
	end
end

function drawgameover()
	cls()
	camera(0,0)
	print_centered("game over!",10,9)
	if p1.lives > p2.lives then
		print_centered("player 1 wins",30, 7)
	else
		print_centered("player 2 wins",30,7)
	end
	print_centered("press ❎ or 🅾️ to start over",70,7)
end
