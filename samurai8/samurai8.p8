--main draw and update
function _update()
	if state=="menu" then
		updatemenu()
	else if state=="game" then
		updategame()
	else if state=="gameover" then
		updategameover()
	end
	end
	end
end

function _draw()
	if state=="menu" then
		drawmenu()
	else if state=="game" then
		drawgame()
	else if state=="gameover" then
		drawgameover()
	end
	end
	end
end
