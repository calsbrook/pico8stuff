pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
player = {
x=0,
y=0,
sprite=1,
isleft=true,
speed=8,
}

function _update() 
	updateplayer()
end

function _draw()
 cls()
 palt(0,false)
 palt(1,true)
 map(0,0)
	spr(player.sprite,player.x,player.y,1,1,player.isleft)
end

function updateplayer()
	local direction=getdir()
	
	if direction=="right" then
		player.isleft=false
		player.sprite=3
		player.x+=player.speed
	elseif direction=="left" then
		player.isleft=true
		player.sprite=3
		player.x-=player.speed
	elseif direction=="up" then
		player.isleft=false
		player.sprite=2
		player.y-=player.speed
	elseif direction=="down" then
		player.isleft=false
		player.sprite=1
		player.y+=player.speed
	else
		return
	end
	
	
end

function getdir()
	if btnp(⬇️) then
		return "down"
	elseif btnp(➡️) then
		return "right"
	elseif btnp(⬆️) then
		return "up"
	elseif btnp(⬅️) then
		return "left"
	end
	
end
